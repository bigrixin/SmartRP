@model SmartRP.Domain.Grade
@switch (@Model)
{
	case SmartRP.Domain.Grade.HighDistinction:
	<span class="label label-success">@Model.ToString()</span>
		break;
	case SmartRP.Domain.Grade.Distinction:
	<span class="label label-info">@Model.ToString()</span>
		break;
	case SmartRP.Domain.Grade.Credit:
	<span class="label label-primary">@Model.ToString()</span>
		break;
	case SmartRP.Domain.Grade.Pass:
	<span class="label label-default">@Model.ToString()</span>
		break;
	case SmartRP.Domain.Grade.FailX:
	<span class="label label-warning">@Model.ToString()</span>
		break;
	case SmartRP.Domain.Grade.Fail:
	<span class="label label-danger">@Model.ToString()</span>
		break;
	default:
	<span class="label label-default">@Model.ToString()</span>
		break;
}
