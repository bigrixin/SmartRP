@model SmartRP.Domain.RequestStatus

@switch (@Model)
{
	case SmartRP.Domain.RequestStatus.Requested:
	<span class="label label-info">@Model.ToString()</span>
		break;
	case SmartRP.Domain.RequestStatus.Accepted:
	<span class="label label-primary">@Model.ToString()</span>
		break;
	case SmartRP.Domain.RequestStatus.Rejected:
	<span class="label label-danger">@Model.ToString()</span>
		break;
	case SmartRP.Domain.RequestStatus.Registered:
	<span class="label label-success">@Model.ToString()</span>
		break;
	case SmartRP.Domain.RequestStatus.None:
	<span class="label label-default">@Model.ToString()</span>
		break;
	case SmartRP.Domain.RequestStatus.Withdraw:
	<span class="label label-warning">@Model.ToString()</span>
		break;
	case SmartRP.Domain.RequestStatus.Deleted:
	<span class="label label-default">@Model.ToString()</span>
		break;
	case SmartRP.Domain.RequestStatus.Quit:
	<span class="label label-default">@Model.ToString()</span>
		break;
}
